pipeline{
    agent any
    stages{
        stage('Git Clone'){
            steps{
                git 'https://github.com/Shivank12345/hello-world-spring-boot.git'
            }
        }
        stage('Test Stage'){
            steps{
                echo "Testing completed"
            }
        }
        stage('Build Stage'){
            steps{
                echo "Build completed"
            }
        }
    }
}
